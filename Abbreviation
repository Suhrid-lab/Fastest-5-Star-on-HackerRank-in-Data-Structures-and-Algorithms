import java.io.*;
import java.util.*;

public class Solution {

    public static String abbreviation(String a, String b) {
        int n = a.length();
        int m = b.length();
        
        boolean[][] dp = new boolean[n + 1][m + 1];
        
        // Base case: An empty 'a' can be abbreviated to an empty 'b'.
        dp[0][0] = true;
        
        // Fill the first column: a can be abbreviated to an empty b if all a's chars are lowercase.
        for (int i = 1; i <= n; i++) {
            if (Character.isLowerCase(a.charAt(i - 1))) {
                dp[i][0] = dp[i - 1][0];
            } else {
                dp[i][0] = false;
            }
        }
        
        // Fill the DP table
        for (int i = 1; i <= n; i++) {
            for (int j = 1; j <= m; j++) {
                char charA = a.charAt(i - 1);
                char charB = b.charAt(j - 1);
                
                if (charA == charB) {
                    // Case 1: Match directly (uppercase)
                    dp[i][j] = dp[i - 1][j - 1];
                } else if (Character.toUpperCase(charA) == charB) {
                    // Case 2: Convert and match (lowercase)
                    // We can either match this char or skip it.
                    dp[i][j] = dp[i - 1][j - 1] || dp[i - 1][j];
                } else if (Character.isLowerCase(charA)) {
                    // Case 3: Skip a lowercase char
                    dp[i][j] = dp[i - 1][j];
                } else {
                    // Case 4: Cannot match or skip uppercase char that doesn't match
                    dp[i][j] = false;
                }
            }
        }
        
        if (dp[n][m]) {
            return "YES";
        } else {
            return "NO";
        }
    }

    public static void main(String[] args) throws IOException {
        BufferedReader bufferedReader = new BufferedReader(new InputStreamReader(System.in));
        BufferedWriter bufferedWriter = new BufferedWriter(new FileWriter(System.getenv("OUTPUT_PATH")));

        int q = Integer.parseInt(bufferedReader.readLine().trim());

        for (int qItr = 0; qItr < q; qItr++) {
            String a = bufferedReader.readLine();
            String b = bufferedReader.readLine();

            String result = abbreviation(a, b);

            bufferedWriter.write(result);
            bufferedWriter.newLine();
        }

        bufferedReader.close();
        bufferedWriter.close();
    }
}
